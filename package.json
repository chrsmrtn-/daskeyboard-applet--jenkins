{
  "name": "daskeyboard-applet--jenkins",
  "displayName": "Jenkins Pipeline",
  "version": "0.0.10",
  "description": "Displays status of Jenkins pipeline.",
  "longDescription": "Monitor the status of your Jenkins pipeline on your Q keyboard!",
  "officialProductName": "Jenkins Pipeline",
  "appUrl": "",
  "isSingleton": false,
  "videoUrl": "",
  "icon": "assets/icon.png",
  "image": "assets/q-jenkins.png",
  "publisher": "chrsmrtn",
  "authorName": "chrsmrtn",
  "authorUrl": "https://github.com/chrsmrtn-",
  "issuesUrl": "https://github.com/chrsmrtn-/daskeyboard-applet--jenkins/issues",
  "homePageUrl": "https://github.com/chrsmrtn-/daskeyboard-applet--jenkins",
  "developerRepoUrl": "https://github.com/chrsmrtn-/daskeyboard-applet--jenkins",
  "licenseUrl": "https://opensource.org/licenses/MIT",
  "changelogUrl": "CHANGELOG.md",
  "license": "MIT",
  "readMeUrl": "README.md",
  "readMeEndUserUrl": "README_ENDUSER.md",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "mocha"
  },
  "engines": {
    "das-keyboard-q": "3.1.1"
  },
  "dependencies": {
    "daskeyboard-applet": "^2.11.4",
    "request": "^2.34",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^7.0.1"
  },
  "qConfig": {
    "geometry": {
      "width": 1,
      "height": 1,
      "defaults": {
        "origin": {
          "x": 0,
          "y": 1
        }
      }
    },
    "authorization": {
      "type": "apiKey",
      "hint": "https://username:token@jenkins.company.com to connect to jenkins server"
    },
    "questions": [
      {
        "key": "pipeline",
        "label": "Choose a pipeline to monitor",
        "help": "Select a pipeline from the list",
        "required": true,
        "order": 1,
        "controlType": "dropdown",
        "dynamic": true,
        "options": []
      },
      {
        "key": "successColor",
        "label": "Choose color for SUCCESS",
        "help": "Color when build was successful",
        "order": 2,
        "value": "#00FF00",
        "controlType": "color"
      },
      {
        "key": "successEffect",
        "label": "Choose a SUCCESS effect",
        "help": "Effect when build was successful",
        "order": 3,
        "value": "SET_COLOR",
        "controlType": "effect"
      },
      {
        "key": "failureColor",
        "label": "Choose a FAILURE color",
        "help": "Color when build was a failure",
        "order": 4,
        "value": "#FF0000",
        "controlType": "color"
      },
      {
        "key": "failureEffect",
        "label": "Choose a FAILURE effect",
        "help": "Effect when build was a failure",
        "order": 5,
        "value": "BLINK",
        "controlType": "effect"
      },
      {
        "key": "buildingColor",
        "label": "Choose a BUILDING color",
        "help": "Color when building",
        "order": 6,
        "value": "#0000FF",
        "controlType": "color"
      },
      {
        "key": "buildingEffect",
        "label": "Choose a BUILDING effect",
        "help": "Effect when build is building",
        "order": 7,
        "value": "BREATHE",
        "controlType": "effect"
      },
      {
        "key": "abortedColor",
        "label": "Choose an ABORTED color",
        "help": "Color when the build aborted",
        "order": 8,
        "value": "#C0C0C0",
        "controlType": "color"
      },
      {
        "key": "abortedEffect",
        "label": "Choose an ABORTED effect",
        "help": "Effect when build aborted",
        "order": 9,
        "value": "SET_COLOR",
        "controlType": "effect"
      }
    ]
  }
}
